              .col-md-6
                .form-group
                  label(for='') Time
                  input#book_time.form-control(type='text' placeholder='Time')
              .col-md-6
                .form-group
                  label(for='') Person
                  .select-wrap.one-third
                    .icon
                      span.ion-ios-arrow-down
                    select.form-control(name='' id='')
                      option(value='') Person
                      option(value='') 1
                      option(value='') 2
                      option(value='') 3
                      option(value='') 4+
              .col-md-12.mt-3
                .form-group
